import io;

class ArrayAccess {

    public int sum(int[] a, boolean b) {

        int l;
        int sum;

        l = a.length;
        sum = 0;

        if (b) {
            while (l > 0) {
                sum = sum + a[l-1];
                l = l - 1;
            }
        }

        return sum;

    }

